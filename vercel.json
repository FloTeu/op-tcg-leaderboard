{
  "version": 2,
  "functions": {
    "api/index.py": {
      "runtime": "now-python@1.0.1"
    }
  },
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/op_tcg/frontend_fasthtml/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ],
  "env": {
    "PYTHONPATH": "."
  }
}
