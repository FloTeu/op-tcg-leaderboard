{
  "version": 2,
  "functions": {
    "api/**/*.py": {
      "runtime": "python3.9"
    }
  },
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/op_tcg/frontend_fasthtml/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ],
  "env": {
    "PYTHONPATH": "."
  }
}
